"routes": [
  {
    "route": "/",
    "allowedRoles": ["authenticated"],
    "rewrite": "/index.html"
  },
  {
    "route": "/",
    "allowedRoles": ["anonymous"],
    "rewrite": "/login.html"
  },
  {
    "route": "/login",
    "serve": "/login.html",
    "allowedRoles": ["anonymous"]
  },
  {
    "route": "/login.html",
    "allowedRoles": ["anonymous"]
  },
  {
    "route": "/.auth/*",
    "allowedRoles": ["anonymous"]
  },
  {
    "route": "/index.html",
    "allowedRoles": ["authenticated"]
  },
  {
    "route": "/*",
    "allowedRoles": ["authenticated"],
    "rewrite": "/index.html"
  }
]
