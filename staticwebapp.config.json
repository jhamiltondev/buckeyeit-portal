{
  "routes": [
    {
      "route": "/",
      "redirect": "/login",
      "statusCode": 302
    },
    {
      "route": "/login",
      "serve": "/login.html"
    },
    {
      "route": "/login.html"
    },
    {
      "route": "/index.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/.auth/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/login.html",
    "exclude": [
      "/login.html",
      "/.auth/*",
      "/css/*",
      "/images/*",
      "/js/*"
    ]
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Cache-Control": "no-cache"
  },
  "platform": {
    "apiRuntime": "node:16"
  }
}
