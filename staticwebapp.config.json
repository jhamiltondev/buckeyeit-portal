{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "serve": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/.auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/index.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Cache-Control": "no-cache"
  },
  "navigationFallback": {
    "rewrite": "/login.html",
    "exclude": ["/login.html", "/.auth/*", "/css/*", "/images/*"]
  },
  "platform": {
    "apiRuntime": "node:16"
  }
}
